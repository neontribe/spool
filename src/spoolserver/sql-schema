/*
Schema
*/

CREATE TABLE users ( id INTEGER PRIMARY KEY AUTOINCREMENT, firstName TEXT, lastName TEXT, email TEXT );
CREATE TABLE entries ( id INTEGER PRIMARY KEY AUTOINCREMENT, mediaId INTEGER UNIQUE, authorId INTEGER, ownerId INTEGER, timestamp DATETIME DEFAULT CURRENT_TIMESTAMP );
CREATE TABLE mediaTypes ( id INTEGER PRIMARY KEY AUTOINCREMENT, type STRING UNIQUE );
-- media values itself may need to be abstracted out
-- makes joins more complicated unless media tables share a common interface
-- would recommend keeping this column fat for now
CREATE TABLE media ( id INTEGER PRIMARY KEY AUTOINCREMENT, typeId INTEGER, text TEXT );
INSERT INTO mediaTypes (type) VALUES ("text"), ("video"), ("image");

