{
  "name": "spool",
  "version": "1.0.0",
  "engines": {
    "node": "6.2.1",
    "npm": "3.10.3"
  },
  "description": "Helping folk with learning disabilities get involved in policy consultations",
  "main": "src/server/index.js",
  "private": true,
  "proxy": "http://localhost:3001",
  "scripts": {
    "test": "NODE_ENV=test && if [ -f .env ] ; then export $(cat .env | xargs) ; fi && nyc mocha .mocha_setup.js src/**/*-test.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint .",
    "start-ui": "if [ -f .env ] ; then export $(cat .env | xargs) ; fi && node scripts/start.js",
    "build": "if [ -f .env ] ; then export $(cat .env | xargs) ; fi && node scripts/build.js && cp -r ./static/* ./build/static/",
    "storybook": "start-storybook -p 9009 -s ./",
    "build-storybook": "build-storybook",
    "start": "node src/server/index.js",
    "start-dev": "if [ -f .env ] ; then export $(cat .env | xargs) ; fi && npm start",
    "build-schema": "if [ -f .env ] ; then export $(cat .env | xargs) ; fi && node ./scripts/updateSchema.js",
    "setup-database": "if [ -f .env ] ; then export $(cat .env | xargs) ; fi && sequelize --config config/database.js --env spool db:seed:all",
    "build-database": "if [ -f .env ] ; then export $(cat .env | xargs) ; fi && sequelize --config config/database.js --env spool db:migrate && npm run setup-database",
    "reset-database": "if [ -f .env ] ; then export $(cat .env | xargs) ; fi && npm run reset-seeds && sequelize --config config/database.js --env spool db:migrate:undo:all",
    "reset-seeds": "if [ -f .env ] ; then export $(cat .env | xargs) ; fi && for f in seeders/*; do sequelize --config config/database.js --env spool --seed \"$f\" db:seed:undo; done",
    "postinstall": "npm run build && npm run build-database",
    "generate-database": "pgen exec ./config/sequelize-template -t src/server/database/models"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neontribe/spool.git"
  },
  "author": "Rupert Redington <rupert@neontribe.co.uk>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/neontribe/spool/issues"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.12.0",
    "babel-jest": "15.0.0",
    "babel-plugin-istanbul": "^2.0.1",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "detect-port": "1.0.1",
    "enzyme": "^2.4.1",
    "http-proxy-middleware": "0.17.1",
    "jest": "15.1.1",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.1",
    "nyc": "^8.1.0",
    "pg-generator": "^3.4.2",
    "react-addons-test-utils": "^15.3.0",
    "supertest": "^2.0.0",
    "webpack-dev-server": "1.16.1"
  },
  "dependencies": {
    "auth0-js": "^7.1.0",
    "autoprefixer": "6.4.0",
    "aws-sdk": "^2.5.2",
    "babel-cli": "^6.11.4",
    "babel-core": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-syntax-async-functions": "6.8.0",
    "babel-plugin-syntax-trailing-function-commas": "6.8.0",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-regenerator": "6.14.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-es2016": "6.11.3",
    "babel-preset-react": "6.11.1",
    "babel-relay-plugin": "^0.9.2",
    "babel-runtime": "6.11.6",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "capture-video-frame": "^0.1.1",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "co": "^4.6.0",
    "compression": "^1.6.2",
    "connect-history-api-fallback": "1.3.0",
    "cors": "^2.7.1",
    "cross-spawn": "4.0.0",
    "css-loader": "0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "3.12.0",
    "eslint-loader": "1.4.1",
    "eslint-plugin-flowtype": "2.18.1",
    "eslint-plugin-import": "1.15.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.2.2",
    "express": "^4.14.0",
    "express-graphql": "^0.5.3",
    "express-jwt": "^5.0.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "fs-extra": "0.30.0",
    "graphql": "^0.7.0",
    "graphql-relay": "^0.4.2",
    "gzip-size": "3.0.0",
    "heroku-ssl-redirect": "0.0.3",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.22.0",
    "json-loader": "0.5.4",
    "jwt-decode": "^2.1.0",
    "lodash": "^4.14.2",
    "moment": "^2.14.1",
    "msr": "^1.3.4",
    "node-uuid": "^1.4.7",
    "object-assign": "4.1.0",
    "opn": "4.0.2",
    "path-exists": "3.0.0",
    "pg": "^6.1.0",
    "postcss-loader": "0.13.0",
    "promise": "7.1.1",
    "react": "^15.2.1",
    "react-bootstrap": "^0.30.2",
    "react-bootstrap-date-picker": "^3.4.0",
    "react-countdown-clock": "^1.0.9",
    "react-dom": "^15.2.1",
    "react-keydown": "^1.6.2",
    "react-relay": "^0.9.2",
    "react-relay-network-layer": "^1.3.3",
    "react-router": "^2.6.1",
    "react-router-relay": "^0.13.4",
    "recursive-readdir": "2.0.0",
    "rimraf": "2.5.4",
    "sequelize": "^3.24.1",
    "sequelize-cli": "^2.4.0",
    "serve-static": "^1.11.1",
    "sha.js": "^2.4.5",
    "sql-template-strings": "^2.2.0",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "sync-request": "^3.0.1",
    "url-loader": "0.5.7",
    "webpack": "^1.13.1",
    "whatwg-fetch": "1.0.0",
    "winston": "^2.2.0"
  },
  "eslintConfig": {
    "extends": "./config/eslint.js"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*-test.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": true
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "jest": {
    "automock": false,
    "moduleNameMapper": {
      "^[./a-zA-Z0-9$_-]+\\.(jpg|png|gif|eot|svg|ttf|woff|woff2|mp4|webm)$": "<rootDir>/config/jest/FileStub.js",
      "^[./a-zA-Z0-9$_-]+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    },
    "persistModuleRegistryBetweenSpecs": true,
    "scriptPreprocessor": "<rootDir>/config/jest/transform.js",
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/environment.js",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/build/"
    ],
    "testRegex": "(__tests__/.*|\\.(test|spec))\\.js$",
    "testEnvironment": "node",
    "verbose": true
  }
}
