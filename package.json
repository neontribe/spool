{
  "name": "spool",
  "version": "1.0.0",
  "description": "Helping folk with learning disabilities get involved in policy consultations",
  "main": "src/server/index.js",
  "private": true,
  "scripts": {
    "test": "mocha .mocha_setup.js src/**/*-test.js --require babel-register",
    "coverage": "babel-node ./node_modules/istanbul/lib/cli cover _mocha -- .mocha_setup.js src/**/*-test.js --require babel-register",
    "ci": "babel-node ./node_modules/istanbul/lib/cli cover _mocha -- .mocha_setup.js src/**/*-test.js --require babel-register --report text-lcov | node ./node_modules/coveralls/bin/coveralls.js && snyk test",
    "start-ui": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "start": "node src/server/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neontribe/spool.git"
  },
  "author": "Rupert Redington <rupert@neontribe.co.uk>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/neontribe/spool/issues"
  },
  "homepage": "https://github.com/neontribe/spool#readme",
  "devDependencies": {
    "@kadira/storybook": "^2.0.0",
    "babel-cli": "^6.11.4",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "enzyme": "^2.4.1",
    "eslint": "^3.2.2",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^9.4.1",
    "mocha": "^3.0.1",
    "react-addons-test-utils": "^15.3.0",
    "react-scripts": "0.2.1",
    "snyk": "^1.17.5",
    "supertest": "^2.0.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "lodash": "^4.14.2",
    "react": "^15.2.1",
    "react-bootstrap": "^0.30.2",
    "react-dom": "^15.2.1"
  },
  "eslintConfig": {
    "extends": "./node_modules/react-scripts/config/eslint.js",
    "env": {
        "mocha": true
    }
  }
}
