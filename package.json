{
  "name": "spool",
  "version": "1.0.0",
  "engines": {
    "node": "6.2.1",
    "npm": "3.10.3"
  },
  "homepage": "./",
  "description": "Helping folk with learning disabilities get involved in policy consultations",
  "main": "src/server/index.js",
  "private": true,
  "proxy": "http://localhost:3001",
  "scripts": {
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint .",
    "start-ui": "if [ -f .env ] ; then export $(cat .env | xargs) ; fi && react-scripts start",
    "start": "node src/server/index.js",
    "start-dev": "if [ -f .env ] ; then export $(cat .env | xargs) ; fi && npm start",
    "setup-database": "if [ -f .env ] ; then export $(cat .env | xargs) ; fi && sequelize --config config/database.js --env spool db:seed:all",
    "build-database": "if [ -f .env ] ; then export $(cat .env | xargs) ; fi && sequelize --config config/database.js --env spool db:migrate && npm run setup-database",
    "reset-database": "if [ -f .env ] ; then export $(cat .env | xargs) ; fi && npm run reset-seeds && sequelize --config config/database.js --env spool db:migrate:undo:all",
    "reset-seeds": "if [ -f .env ] ; then export $(cat .env | xargs) ; fi && for f in seeders/*; do sequelize --config config/database.js --env spool --seed \"$f\" db:seed:undo; done",
    "postinstall": "npm run build && npm run build-database",
    "generate-database": "pgen exec ./config/sequelize-template -t src/server/database/models"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neontribe/spool.git"
  },
  "author": "Rupert Redington <rupert@neontribe.co.uk>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/neontribe/spool/issues"
  },
  "devDependencies": {
    "@neontribe/react-scripts": "^0.8.0",
    "detect-port": "1.0.1",
    "enzyme": "^2.4.1",
    "http-proxy-middleware": "0.17.1",
    "lodash": "^4.17.2",
    "mocha": "^3.0.1",
    "nyc": "^8.1.0",
    "pg-generator": "^3.4.2",
    "react-addons-test-utils": "^15.3.0",
    "supertest": "^2.0.0"
  },
  "dependencies": {
    "auth0-js": "^7.1.0",
    "autoprefixer": "6.4.0",
    "aws-sdk": "^2.5.2",
    "babel-relay-plugin": "^0.9.2",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "capture-video-frame": "^0.1.1",
    "chalk": "1.1.3",
    "co": "^4.6.0",
    "compression": "^1.6.2",
    "connect-history-api-fallback": "1.3.0",
    "cors": "^2.7.1",
    "cross-spawn": "4.0.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.4.1",
    "express": "^4.14.0",
    "express-graphql": "^0.5.3",
    "express-jwt": "^5.0.0",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "fs-extra": "0.30.0",
    "graphql": "^0.7.0",
    "graphql-relay": "^0.4.2",
    "gzip-size": "3.0.0",
    "heroku-ssl-redirect": "0.0.3",
    "json-loader": "0.5.4",
    "jwt-decode": "^2.1.0",
    "lodash": "^4.14.2",
    "moment": "^2.14.1",
    "msr": "^1.3.4",
    "node-uuid": "^1.4.7",
    "object-assign": "4.1.0",
    "opn": "4.0.2",
    "papaparse": "^4.1.2",
    "path-exists": "3.0.0",
    "pg": "^6.1.0",
    "postcss-loader": "0.13.0",
    "promise": "7.1.1",
    "random-seed": "^0.3.0",
    "react": "^15.2.1",
    "react-bootstrap": "^0.30.2",
    "react-bootstrap-date-picker": "^3.4.0",
    "react-dom": "^15.2.1",
    "react-keydown": "^1.6.2",
    "react-relay": "^0.9.2",
    "react-relay-network-layer": "^1.3.3",
    "react-router": "^2.6.1",
    "react-router-relay": "^0.13.4",
    "recursive-readdir": "2.0.0",
    "rimraf": "2.5.4",
    "sequelize": "^3.24.1",
    "sequelize-cli": "^2.4.0",
    "serve-static": "^1.11.1",
    "sha.js": "^2.4.5",
    "shuffle-array": "^1.0.0",
    "sql-template-strings": "^2.2.0",
    "strip-ansi": "3.0.1",
    "style-loader": "^0.13.1",
    "sync-request": "^3.0.1",
    "viewport-units-buggyfill": "^0.6.0",
    "whatwg-fetch": "1.0.0",
    "winston": "^2.2.0"
  },
  "eslintConfig": {
    "extends": "@neontribe/eslint-config-react-app"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/*-test.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": true
  }
}
